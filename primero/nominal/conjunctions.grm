package primero

general construction Conjunction
	subcase of Word

construction And-Serial
	subcase of SerialConjunction
	form
	  constraints
	    self.f.orth <-- "y"
	    
construction Then-Serial
	subcase of SerialConjunction
	form
	  constraints
	    self.f.orth <-- "entonces" 

general construction SerialConjunction
	subcase of Conjunction	

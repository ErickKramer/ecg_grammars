package robots
import core
//import transitives




construction ReturnType
	subcase of LexicalVerbType
	meaning: MotionPath
	 constraints
	   self.m.actionary <-- @move
	   self.m.spg.goal <-- @home



construction Move2
	subcase of LexicalVerb, PastTense
	form
	   constraints
	      self.f.orth <-- "moved"
	meaning: Motion
	 constraints
	   self.m.actionary <-- @move

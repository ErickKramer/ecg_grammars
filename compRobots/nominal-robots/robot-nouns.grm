package robots
import starter

	
construction Side
	subcase of SingularNoun
	form
	  constraints
	    self.f.orth <-- "side"
	meaning: RD
	  evokes PartWhole as pw
	    constraints
	      self.m <--> pw.part
	      self.m.ontological-category <-- @side
	      self.m.extensions <--> pw
	      
	      